defined_text = {
    name = GetMilitaryYLabel1

    text = {
        trigger = { 
                set_temp_variable = { score = 100 }
                mulitply_temp_variable = {
                    score = GLOBAL.ASG_military_rendering_scale_factor
                }
        }
        localization_key = "[?score]"
    }
}

